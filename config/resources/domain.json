{
  "version": "0.1",
  "prefixes": {
    "schema": "http://schema.org/",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "totlg": "http://data.totl.net/game/"
  },
  "resources": {
    "game": {
      "class": "totlg:Game",
      "path": "games",
      "new-resource-base": "ext:game/",
      "attributes": {
        "user": {
          "type": "string",
          "predicate": "foaf:accountName"
        },
        "step": {
          "type": "number",
          "predicate": "ext:step"
        },
        "page": {
          "type": "string",
          "predicate": "ext:fixme"
        }
      }
    },
    "wiki-links-to": {
      "name": "wiki-links-to",
      "path": "about",
      "class": "schema:WebApi",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "schema:name",
        },
        "status": {
          "type": "schema:GameServerStatus",
          "predicate": "schema:serverStatus",
        }
      },
      "new-resource-base": "http://mu.semte.ch/application"
    }
  }
}
